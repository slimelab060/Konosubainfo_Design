{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  
    "extends": ["@nuxtjs"],
    "timezone": "Asia/Tokyo",
    "schedule": ["before 10am"],
    "labels": ["renovate"],
    "prConcurrentLimit": 5,
    "dependencyDashboard": false,
    "automerge": true,
    
    "lockFileMaintenance": {
        "enabled": true
      },

    "packageRules": [
        {
          "packagePatterns": ["^swiper"],
          "enabled": false
        }
    ]
  }
  